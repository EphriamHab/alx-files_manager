{
  "name": "alx-files_manager",
  "version": "1.0.0",
  "description": "File manager project",
  "main": "server.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "lint-nt": "./node_modules/.bin/eslint.cmd",
    "check-lint": "lint controllers/ libs/ middlewares/ routes/ utils/ server.js worker.js",
    "check-lint-nt": "yarn lint-nt controllers/ libs/ middlewares/ routes/ utils/ server.js worker.js",
    "start-server": "nodemon --exec babel-node --presets @babel/preset-env ./server.js",
    "start-worker": "nodemon --exec babel-node --presets @babel/preset-env ./worker.js",
    "dev": "nodemon --exec babel-node --presets @babel/preset-env",
    "test": "./node_modules/.bin/mocha --opts mocha.opts tests/**/*.js",
    "test-with-coverage": "nyc --reporter=text ./node_modules/.bin/mocha --opts mocha.opts tests/**/*.js",
    "coveralls": "nyc ./node_modules/.bin/mocha --opts mocha.opts tests/**/*.js && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://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@github.com/EphriamHab/alx-files_manager.git"
  },
  "author": "Ephrem Habtamu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EphriamHab/alx-files_manager/issues"
  },
  "homepage": "https://github.com/EphriamHab/alx-files_manager#readme",
  "devDependencies": {
    "eslint": "^8.54.0"
  },
  "dependencies": {
    "bull": "^4.11.5",
    "chai-http": "^4.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "image-thumbnail": "^1.0.15",
    "mime-message": "^0.1.3",
    "mime-types": "^2.1.35",
    "mongodb": "^6.3.0",
    "redis": "^4.6.11",
    "sha1": "^1.1.1",
    "uuid": "^9.0.1"
  }
}
